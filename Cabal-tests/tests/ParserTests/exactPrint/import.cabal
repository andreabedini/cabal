cabal-version:           3.0
name:                    two-sections
version:                 0
synopsis:                The -any none demo
build-type:              Simple


common common-options
  default-extensions: 
       GADTs
  ghc-options: -Wall -Wcompat -Wnoncanonical-monad-instances -Wincomplete-uni-patterns -Wincomplete-record-updates

library
  import: common-options
  default-language:  Haskell2010
  exposed-modules:   AnyNone
  build-depends:     base <5,
                     containers

executable two
  default-language:  Haskell2010
  main-is: main.hs
  build-depends:     base <5,
                     containers >3,
                     two-sections